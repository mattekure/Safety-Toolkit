<?xml version="1.0" encoding="iso-8859-1"?>

<root>
	<windowclass name="SafetyToolkitWindow">
		<frame>safetybox</frame>
		<placement>
			<size width="170" height="160" />
		</placement>
		<sizelimits>
			<minimum width="170" height="160" />
		</sizelimits>
		<sheetdata>
			<anchor_column name="contentanchor" />
			<close_utilitybox />
			<safetyCard name="card_x">
				<anchored>
					<top parent="contentanchor" anchor="bottom" relation="current" offset="50" />
					<left offset="20" />
				</anchored>
			</safetyCard>
			<safetyCard name="card_n">
				<anchored>
					<top parent="contentanchor" anchor="bottom" relation="current" offset="50" />
					<left offset="65" />
				</anchored>
			</safetyCard>
			<safetyCard name="card_o">
				<anchored>
					<top parent="contentanchor" anchor="bottom" relation="current" offset="50" />
					<left offset="110" />
				</anchored>
			</safetyCard>
			<simplenumberc name="number_x">
					<anchored height="20" width="40">
					<top parent="contentanchor" anchor="bottom" relation="current" offset="120" />
					<left offset="20" />
				</anchored>	
				<script>
					function onInit()
						sName = getName();
						local num = SafetyToolkit.checkUnack(sName);
						setValue(num);
					end
				</script>
				<center />
				<readonly />
				<gmvisibleonly />
			</simplenumberc>
			<simplenumberc name="number_n">
					<anchored height="20" width="40">
					<top parent="contentanchor" anchor="bottom" relation="current" offset="120" />
					<left offset="65" />
				</anchored>	
				<script>
					function onInit()
						sName = getName();
						local num = SafetyToolkit.checkUnack(sName);
						setValue(num);
					end
				</script>
				<center />
				<readonly />
				<gmvisibleonly />
			</simplenumberc>
			<simplenumberc name="number_o">
					<anchored height="20" width="40">
					<top parent="contentanchor" anchor="bottom" relation="current" offset="120" />
					<left offset="110" />
				</anchored>	
				<script>
					function onInit()
						sName = getName();
						local num = SafetyToolkit.checkUnack(sName);
						setValue(num);
					end
				</script>
				<center />
				<readonly />
				<gmvisibleonly />
			</simplenumberc>
		</sheetdata>
	</windowclass>	

</root>